if [[ "$HK" == "1" ]]; then echo " * Thêm Harman Kardon..."
mkdir -p $TOME/tam
7za x -tzip -y $TOME/.github/HarmanKardon.zip -o$TOME/tam >/dev/null
sudo cp -af $TOME/tam/system/vendor/* $phanvungtam/vendor 
sudo rm -rf $TOME/tam
fi 
